{
  "name": "WebserverFramework",
  "version": "0.1.0",
  "description": "Webserver Framework",
  "scripts": {
    "dev": "npm run lint -s && concurrently --kill-others 'npm run dev:client -s'",
    "dev:client": "node client-side-dev-server.js",
    "build": "rimraf dist && npm run lint -s && webpack --config webpack.production.config.js",
    "doc": "documentation build test/** -f html -o doc",
    "lint": "eslint .",
    "test": "qunit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/llovell/webserver-framework"
  },
  "author": "Llewellyn Lovell",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "concurrently": "^3.4.0",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0",
    "qunitjs": "^2.3.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.1",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.4.5"
  }
}
